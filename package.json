{
  "name": "webos",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --ignore-pattern 'cypress/*' && tsc --noEmit --skipLibCheck",
    "stylelint": "stylelint **/*.scss",
    "prettier": "prettier --write .",
    "prepare": "husky",
    "test": "vitest --run",
    "test:w": "vitest --watch",
    "test:current": "vitest use-processes.test",
    "cy:window": "cypress run --spec 'cypress/e2e/window.cy.ts'",
    "test:ui": "vitest --ui"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{ts,tsx}": "stylelint --fix --allow-empty-input",
    "*.{js,ts,tsx}": "eslint --ignore-pattern 'cypress/**' --fix"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "fast-deep-equal": "^3.1.3",
    "immer": "^10.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.3.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.11.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.4",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-compiler": "^0.0.0-experimental-51a85ea-20240601",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-tailwindcss": "^3.15.2",
    "eslint-plugin-unicorn": "^53.0.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "prettier": "^3.2.5",
    "sass": "^1.77.2",
    "stylelint": "^16.5.0",
    "stylelint-config-sass-guidelines": "^11.1.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^1.6.0"
  }
}
