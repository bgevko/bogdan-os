{
  "name": "webos",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "BUILD_ENV=production vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --ignore-pattern 'cypress/*'; tsc --noEmit --skipLibCheck",
    "tsc": "tsc --noEmit --skipLibCheck",
    "stylelint": "stylelint **/*.css",
    "prettier": "prettier --write .",
    "prepare": "husky",
    "test": "vitest --run",
    "test:w": "vitest --watch",
    "test:current": "vitest file-system.test.tsx",
    "test:ui": "vitest --ui"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{ts,tsx}": "stylelint --fix --allow-empty-input",
    "*.{js,ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.17.6",
    "clsx": "^2.1.1",
    "immer": "^10.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.3.0",
    "three": "^0.169.0",
    "uuid": "^11.0.3",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.14.0",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@types/three": "^0.175.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.4",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-compiler": "^19.0.0-beta-55955c9-20241229",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-tailwindcss": "^3.15.2",
    "eslint-plugin-unicorn": "^53.0.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "5.5.4",
    "vite": "^5.2.0",
    "vite-plugin-glsl": "^1.3.0",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^1.6.0"
  },
  "resolutions": {
    "typescript": "5.5.4"
  },
  "packageManager": "yarn@4.8.1"
}
